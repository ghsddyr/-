#include <stdio.h>
#define row 5
#define col 5
int z[row][col];
int visited[row][col] = { 0 };
    void printz(){
        for (int i = 0; i < row; i++) {
            for (int j = 0; j < col; j++) {
                printf("%d", z[i][j]);
            }
            printf("\n");
        }
    }
        int solvez(int x, int y) {
            if (x < 0 || x >= row || y < 0 || y >= col) return 0;
            if (z[x][y] == 3) {
                z[x][y] = 9;
                return 1;
            }
            if (z[x][y] == 1 || visited[x][y]) return 0;
            visited[x][y] = 1;
            z[x][y] = 9;
            if (solvez(x - 1, y) || solvez(x + 1, y) || solvez(x, y - 1) || solvez(x, y + 1)) {
                return 1;
            }
            z[x][y]=0;
                return 0;
        }
        int main() {
            for (int i = 0; i < row; i++) {
                for (int j = 0; j < col; j++) {
                    scanf_s("%d", &z[i][j],sizeof(z[i][j]));
                }
            }
        int startx=-1,starty=-1;
            for (int i = 0; i < row; i++) {
                for (int j = 0; j < col; j++) {
                    if (z[i][j] == 2) {
                        startx = i;
                        starty = j;
                        break;
                    }
                }
            }
        if (startx == -1 || starty == -1) {
                printf("nothing thereï¼\n");
                return 1;
            }
        if (solvez(startx, starty)) {
            printf("the road:\n");
            printz();
        }
        else {
            printf("something error!\n");
        }
    return 0;
}
